# Performance
VITE_ENABLE_ANALYTICS="true"
VITE_ENABLE_ERROR_REPORTING="false"
VITE_BUNDLE_ANALYZER="false"

# Development
VITE_DEV_PORT="3000"
VITE_DEV_HOST="true"
VITE_DEV_CORS="true"

# MaycoleTechnologies Integration
VITE_MAYCOLE_WEBSITE="https://maycoletechnologies.com"
VITE_MAYCOLE_BRAND="MaycoleTechnologies™"
VITE_SUPPORT_EMAIL="support@maycoletechnologies.com"

# Subscription Configuration
VITE_FREE_TRIAL_DAYS="40"
VITE_PROFESSIONAL_PRICE="89"
VITE_ENTERPRISE_PRICE="199"

# Industry Support
VITE_SUPPORTED_INDUSTRIES="15"
VITE_CUSTOM_INDUSTRY_SUPPORT="true"

# Security
VITE_SECURE_MODE="true"
