# MaycoleTracker™ Volume XI - Environment Configuration

# Application Information
VITE_APP_NAME="MaycoleTracker™ Volume XI"
VITE_APP_VERSION="11.0.1"
VITE_APP_DESCRIPTION="World's most advanced inventory management system"

# Environment
NODE_ENV="production"
VITE_NODE_ENV="production"

# Build Configuration
VITE_BUILD_TARGET="esnext"
VITE_BUILD_SOURCEMAP="false"

# PWA Configuration
VITE_PWA_NAME="MaycoleTracker™ Volume XI"
VITE_PWA_SHORT_NAME="MaycoleTracker™"
VITE_PWA_THEME_COLOR="#007BFF"
VITE_PWA_BACKGROUND_COLOR="#ffffff"

# Features
VITE_ENABLE_PWA="true"
VITE_ENABLE_VOICE_CONTROL="true"
VITE_ENABLE_CAMERA_CAPTURE="true"
